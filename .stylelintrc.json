{
  "customSyntax": "postcss-scss",
  "rules": {
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": [
          "tailwind",
          "layer",
          "apply",
          "respond-to",
          "screen",
          "import",
          "font-face",
          "keyframes",
          "media",
          "supports"
        ]
      }
    ],
    "declaration-block-no-redundant-longhand-properties": null,
    "no-descending-specificity": null,
    "function-no-unknown": null,
    "property-no-unknown": null
  }
}
